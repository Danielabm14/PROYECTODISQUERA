<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">

   <display-name>Disquera</display-name>

<welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>controller.albumController</servlet-name>
     <servlet-class>controller.albumController</servlet-class> 
  </servlet>
<servlet-mapping>
    <servlet-name>controller.albumController</servlet-name>
    <url-pattern>/albumController</url-pattern>
</servlet-mapping>


<servlet>
    <servlet-name>controller.disqueracontroller</servlet-name>
     <servlet-class>controller.disqueracontroller</servlet-class> 
  </servlet>
<servlet-mapping>
    <servlet-name>controller.disqueracontroller</servlet-name>
    <url-pattern>/disqueracontroller</url-pattern>
</servlet-mapping>

<servlet>
    <servlet-name>controller.artistacontroller</servlet-name>
     <servlet-class>controller.artistacontroller</servlet-class> 
  </servlet>
<servlet-mapping>
    <servlet-name>controller.artistacontroller</servlet-name>
    <url-pattern>/artistacontroller</url-pattern>
</servlet-mapping>
</web-app>
